{
  "name": "file-input-directive",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "transpile": "ngc",
    "package": "rollup -c",
    "minify": "uglifyjs dist/bundles/fileinput.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/fileinput.umd.min.js",
    "build": "rimraf dist && tsc -p tsconfig.json && rollup -c rollup.config.js dist/file-input.module.js > dist/file-input.bundle.js && cp package.json dist && ngc && cp readme.md dist"
  },
  "private": true,
  "dependencies": {
    "@angular/forms": "^4.2.4"
  },
  "devDependencies": {
    "@angular/cli": "1.4.4",
    "@angular/compiler": "^4.4.5",
    "@angular/compiler-cli": "^4.4.5",
    "@angular/core": "^4.4.5",
    "rollup": "^0.50.0",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "^2.3.4",
    "uglify-js": "^3.1.3"
  },
  "main": "file-input.bundle.js",
  "module": "file-input.module.js",
  "jsnext:main": "file-input.module.js",
  "types": "file-input.module.d.ts"
}
