  },
  "devDependencies": {
    "@angular/cli": "1.4.4",
    "@angular/compiler": "^4.4.5",
    "@angular/compiler-cli": "^4.4.5",
    "rollup": "^0.50.0",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "^2.3.4",
    "uglify-js": "^3.1.3"
  }
}
